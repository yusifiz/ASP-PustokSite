<section class="hero-area hero-slider-1">
    <div class="sb-slick-slider" data-slick-setting='{
                            "autoplay": true,
                            "fade": true,
                            "autoplaySpeed": 3000,
                            "speed": 3000,
                            "slidesToShow": 1,
                            "dots":true
                            }'>
        @foreach (Slider item in Model.Sliders)
        {
            <div class="single-slide bg-shade-whisper  ">
                <div class="container">
                    <div class="home-content text-center text-sm-left position-relative">
                        <div class="hero-partial-image image-right">
                            <img src="~/assets/image/bg-images/@item.Image" alt="">
                        </div>
                        <div class="row no-gutters ">
                            <div class="col-xl-6 col-md-6 col-sm-7">
                                <div class="home-content-inner content-left-side">
                                    <h1>
                                        @item.Title1<br>
                                        @item.Title2
                                    </h1>
                                    <h2>
                                        @item.Text
                                </h2>
                                <a href="@item.BtnUrl" target="_blank" class="btn btn-outlined--primary">
                                    @item.BtnText
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }


    </div>
</section>
